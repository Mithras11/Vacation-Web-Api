      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\Kaloyan\Desktop\Vacation\Vacation.Web
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Kaloyan\Desktop\Vacation
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Kaloyan\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Kaloyan\.nuget\packages\
